<view class="all"><view class="status_bar"></view><view class="addpsd"><text>新增密码</text></view><view class="editForm"><view class="edittype"><text class="typelabel">请选择类型</text><picker mode="selector" value="{{type}}" range-key="Name" range="{{expressTemplate}}" data-event-opts="{{[['change',[['expressTemplateChange',['$event']]]]]}}" bindchange="__e"><view>{{expressTemplate[type].Name}}</view></picker></view></view><view class="editForm"><input class="titleinput" type="text" placeholder="请输入标题" data-event-opts="{{[['input',[['titlefn',['$event']]]]]}}" value="{{title}}" bindinput="__e"/></view><view class="editForm" style="height:200rpx;"><input class="titleinput" style="border-bottom:#D3D3D3 2rpx solid;" type="text" placeholder="输入用户名" data-event-opts="{{[['input',[['usernamefn',['$event']]]]]}}" value="{{username}}" bindinput="__e"/><input class="titleinput" type="text" placeholder="请输入密码" data-event-opts="{{[['input',[['passwordfn',['$event']]]]]}}" value="{{password}}" bindinput="__e"/></view><view class="editForm" style="height:200rpx;"><textarea class="titleinput" style="overflow:hidden;height:200rpx;line-height:50rpx;" cols="2" rows="6" placeholder="请输入备注" data-event-opts="{{[['input',[['ramarkfn',['$event']]]]]}}" value="{{ramark}}" bindinput="__e"></textarea></view><view data-event-opts="{{[['tap',[['submitbtn',['$event']]]]]}}" class="sumbitbtn" bindtap="__e">添加密码</view></view>